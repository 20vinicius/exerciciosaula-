
public class OperadoresRelacionais {

	public static void main(String[] args) {
		System.out.println(1==1); //relacional de == igual ao numero
		System.out.println(1!=2); //relacional de != diferente ao numero
		System.out.println(1 > 2);//relacional de > maior ao numero comparado
		System.out.println(1 >= 2);//relacional de >= maior ou igual ao numero comparado
		System.out.println(1 < 2);//relacional de < menor ao numero comparado
		System.out.println(1 >= 2);//relacional de >= menor ou igual ao numero comparado
	}

}
