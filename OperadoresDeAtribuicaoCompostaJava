
public class OperadoresDeAtribuicaoComposta {

	public static void main(String[] args) {
		
		int num = 10;
		
		num += 3; // equivalente a num = num(10)+3=13
		System.out.println(num);
		
		num -= 2; //equivalente a num = num(10)-2=11
		System.out.println(num);
		
		num *=5;
		System.out.println(num);
		
		num /= 2;
		System.out.println(num);
