
public class ComandoDeSelecao {

	public static void main(String[] args) {
		int escolha = 5;
		switch(escolha) {
		case 1:
			System.out.println("Numero 1");
			break;
		case 2:
			System.out.println("Numero 2");
		case 3:
			System.out.println("Numero 3");
		default:
			System.out.println("outro numero");
		
		}

	}

}
